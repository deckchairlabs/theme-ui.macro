{
  "name": "theme-ui.macro",
  "version": "0.0.0-experimental.0",
  "description": "Babel macro for transforming ThemeUI spec themes",
  "main": "dist/theme-ui.macro.cjs.js",
  "module": "dist/theme-ui.macro.esm.js",
  "keywords": [
    "babel",
    "babel-plugin-macros",
    "theme-ui",
    "theme"
  ],
  "homepage": "https://github.com/deckchairlabs/theme-ui.macro",
  "bugs": "https://github.com/deckchairlabs/theme-ui.macro/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/deckchairlabs/theme-ui.macro.git"
  },
  "files": [
    "dist/",
    "macro/",
    "plugins/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "preconstruct dev",
    "build": "preconstruct build",
    "release": "npm run test && npm run build && npm publish",
    "test": "yarn jest"
  },
  "author": "James Edmonds",
  "license": "MIT",
  "dependencies": {
    "postcss": "^8.1.9",
    "postcss-nested": "^5.0.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/traverse": "^7.12.9",
    "@babel/types": "^7.12.7",
    "@theme-ui/css": "^0.4.0-rc.11",
    "babel-plugin-macros": "^2.8.0",
    "typescript": "^4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@preconstruct/cli": "^2.0.0",
    "@theme-ui/css": "^0.4.0-rc.11",
    "@types/babel-plugin-macros": "^2.8.4",
    "@types/babel__core": "^7.1.12",
    "@types/babel__traverse": "^7.0.15",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-tester": "^10.0.0",
    "jest": "26.6.0",
    "postcss": "^8.1.9",
    "ts-jest": "^26.4.3",
    "typescript": "^4.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
      "macro.ts",
      "plugins/generateStylesheet.ts",
      "plugins/customProperties.ts",
      "plugins/typescriptDeclarations.ts"
    ]
